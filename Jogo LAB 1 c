#include <stdio.h>
#include <stdbool.h>
bool mover_cima(int mat[4][4]){
  int i =0 , j = 0;
  int aux;
    
   for(int i = 0; i < 4; i++ ){
      for(int j = 0; j < 4; j++){
         if(mat[i][j] == 0 && mat[i][j] != mat[0][j] ){
            aux = mat[i-1][j];
             mat[i-1][j] = mat[i][j];
               mat[i][j] = aux;
            }else{
            
        }
    }
 }
}

bool mover_baixo(int mat[4][4]){
 int i =0 , j = 0;
  int aux;
    
   for(int i = 0; i < 4; i++ ){
      for(int j = 0; j < 4; j++){
         if(mat[i][j] == 0 && mat[i][j] != mat[3][j] ){
            aux = mat[i+1][j];
             mat[i+1][j] = mat[i][j];
               mat[i][j] = aux;
            }else{
            
        }
    }
 }
    
}

void imprimir_matriz( int matriz[4][4]){
 
 for(int i = 0; i < 4; i++ ){
    for(int j = 0; j < 4; j++){
        printf(" %d ", matriz[i][j]);
    }
    printf("\n"); 
 }    
}

int main(){
  
  int matriz[4][4] = {
        {1, 2, 3, 4},
        {5, 0, 7, 8},
        {9, 10, 11, 12},
        {13, 14, 6, 15}
    };


    imprimir_matriz(matriz);
    printf("\n"); 
    mover_cima(matriz);
    printf("\n"); 
    imprimir_matriz(matriz);
      printf("\n"); https
      mover_baixo(matriz);
         imprimir_matriz(matriz);
}
